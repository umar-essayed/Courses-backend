{
  "name": "sinceides-platform",
  "version": "2.0.0",
  "description": "Sinceides LMS Platform - Advanced Learning Management System with Supabase and Cloudinary",
  "main": "api/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon api/index.ts",
    "build": "tsc",
    "seed": "ts-node api/index.ts --seed-only",
    "snapshot": "ts-node api/index.ts --snapshot",
    "restore-snapshot": "ts-node api/index.ts --restore-from-snapshot",
    "vercel-build": "npm run build && npm prune --production",
    "vercel-dev": "vercel dev",
    "vercel-deploy": "vercel --prod",
    "vercel-preview": "vercel",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint api/**/*.ts",
    "lint:fix": "eslint api/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "security-audit": "npm audit",
    "security-fix": "npm audit fix",
    "env:pull": "vercel env pull .env.local",
    "env:push": "vercel env push .env.local"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "@upstash/redis": "^1.35.3",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.14.0",
    "sharp": "^0.33.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.19",
    "@types/helmet": "^4.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.2",
    "@types/passport": "^1.0.14",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/pdfkit": "^0.12.9",
    "@types/sharp": "^0.32.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "lms",
    "learning-management-system",
    "education",
    "online-courses",
    "supabase",
    "cloudinary",
    "typescript",
    "express",
    "jwt",
    "video-streaming",
    "file-upload",
    "security"
  ],
  "author": "Sinceides Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/sinceides-backend.git"
  },
